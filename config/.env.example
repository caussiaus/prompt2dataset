# Database Configuration
DB_PASSWORD=your_secure_password_here
DB_HOST=postgres
DB_PORT=5432
DB_NAME=app_db
DB_USER=postgres

# Ollama LLM Configuration
OLLAMA_HOST=0.0.0.0:11434
OLLAMA_MODELS=mistral:latest,neural-chat:latest,qwen:14b
OLLAMA_URL=http://ollama:11434

# Domain Configuration
DOMAIN=yourdomain.com

# API Keys
API_KEY_SEARXNG=your_api_key_here

# Logging
LOG_LEVEL=info

# n8n Configuration
N8N_SECURE_COOKIE=false
N8N_USER_MANAGEMENT_DISABLED=true

# Service URLs (Internal Docker Network)
HTML_PARSER_URL=http://html-parser:5000
EXTRACTION_AGENT_URL=http://extraction-agent:8001
VISION_AGENT_URL=http://vision-agent:8002
ORCHESTRATOR_AGENT_URL=http://orchestrator-agent:8003
DISCOVERY_AGENT_URL=http://discovery-agent:8004
AGENT_GATEWAY_URL=http://agent-gateway:8000
SEARXNG_URL=http://searxng:8888
CAMOUFOX_URL=http://camoufox:3000

# Frontend Configuration (if applicable)
REACT_APP_API_URL=http://localhost:8000
REACT_APP_GATEWAY_URL=http://localhost:8000

# Service Ports
PORT_POSTGRES=5432
PORT_OLLAMA=11434
PORT_CAMOUFOX=3000
PORT_HTML_PARSER=5000
PORT_SEARXNG=8888
PORT_N8N=5678
PORT_GATEWAY=8000
PORT_EXTRACTION=8001
PORT_VISION=8002
PORT_ORCHESTRATOR=8003
PORT_DISCOVERY=8004

# Flask Configuration
FLASK_ENV=production

# Camoufox Configuration
HEADLESS=true

# Service Configuration Path
SERVICES_CONFIG=/app/services.json
